generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Grade {
  FIRST_YEAR
  SECOND_YEAR
  THIRD_YEAR
  FOURTH_YEAR
}
model Student {
  id         Int      @id @default(autoincrement())
  name       String
  grade      Grade
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
}